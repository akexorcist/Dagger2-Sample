<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akexorcist.dagger2">

    <application
        android:name=".AwesomeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MainActivity" />
        <activity android:name=".ui.home.HomeActivity" />
        <activity android:name=".ui.feed.FeedActivity" />
        <activity android:name=".ui.menu.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.about.AboutActivity" />
        <activity android:name=".ui.detail.DetailActivity" />
        <activity android:name=".ui.payment.PaymentActivity"></activity>
    </application>

</manifest>